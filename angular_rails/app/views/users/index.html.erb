<h1>Registration</h1>
<form >
    <input type="text" ng-model="newUser.user.name" value="name">
    <input type="email" ng-model="newUser.user.email" >
    <input type="password" ng-model="newUser.user.password" value="password">
    <input type="password" ng-model="newUser.user.password_confirmation" value="password">
    <input type="submit" value="Register" ng-click="createUser()">
</form>

<h1>Log in</h1>
<form action="'/'" method="post">
    <input type="email" name="email" >
    <input type="password" name="password" value="password">
    <input type ="submit" value="Login" ng-click="Login()"
</form>
